generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id Int @id @default(autoincrement())
  name String
  species String
  affiliation affiliation
  weapon String
}

enum affiliation {
  REBEL_ALLIANCE 
  EMPIRE 
}
